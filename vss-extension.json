{
    "manifestVersion": 1,
    "id": "file-comparison",
    "name": "File Comparison",
    "version": "0.1.2",
    "publisher": "fthkucuk",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Deploying only different or new files.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/filecomparison.png"
    },
    "files": [
        {
            "path": "filecomparison"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
    "contributions": [
        {
            "id": "filecomparison",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "filecomparison"
            }
        }
    ]
}